{
    "name": "open-orchestra/open-orchestra-model-bundle",
    "description": "An implemantation of the model-interface for OpenOrchestra on mongo",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "OpenOrchestra community",
            "homepage": "https://github.com/open-orchestra/open-orchestra-model-bundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "open-orchestra/open-orchestra-model-interface": "*",
        "doctrine/data-fixtures":"1.1.1",
        "doctrine/mongodb-odm": "1.0.0-BETA13",
        "doctrine/mongodb-odm-bundle" : "3.0.0",
        "solution/mongo-odm-aggregation-bundle": "*",
        "stof/doctrine-extensions-bundle": "1.1.0",
        "antimattr/mongodb-migrations": "~1.0@stable",
        "antimattr/mongodb-migrations-bundle": "~1.0@stable"
    },
    "require-dev": {
        "open-orchestra/open-orchestra-base-bundle": "dev-master",
        "psr/log": "1.0.0",
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/validator": "2.7.0",
        "jmikola/geojson": "1.0.1",
        "phpunit/phpunit" : "4.7.2",
        "phake/phake": "2.1.0"
    },
    "config" : {
        "bin-dir" : "bin"
    },
    "autoload": {
        "psr-0": {
            "OpenOrchestra\\ModelBundle": ""
        }
    },
    "target-dir": "OpenOrchestra"
}
